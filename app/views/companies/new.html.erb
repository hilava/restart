<section id="new_company">
  <%= form_for @company do |f| %>
      <div class="row">
        <div class="well col-md-4 col-md-offset-4">
          <div class="form-group">
            <%= f.text_field :name, class: "form-control", placeholder: "Name", autofocus: true %>
          </div>
          <div class="form-group">
            <%= f.text_field :description, class: "form-control", placeholder: "Description"%>
          </div>
          <div class="form-group">
            <%= f.text_field :website, class: "form-control", placeholder: "Website"%>
          </div>
          <div class="form-group">
            <%= f.text_field :logo, class: "form-control", placeholder: "Logo"%>
          </div>
        <div>
          <center><%= f.submit "Save", class: "btn btn-success" %></center>
        </div>
      </div>
    <% end %>
  </div>
<section>
